{
  "name": "catotel-dashboard",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname localhost --port 3100",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings=0",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test:e2e:web": "playwright test",
    "test:e2e:web:ci": "start-server-and-test \"npm run start\" http://localhost:3000 \"E2E_BASE_URL=http://localhost:3000 npm run test:e2e:web\"",
    "test:e2e:web:ci:full": "start-server-and-test \"npm run start:prod --prefix ../catotel-api\" http://localhost:3333/health \"API_BASE_URL=http://localhost:3333/api/v1 npm run test:e2e:web:ci\""
  },
  "dependencies": {
    "@catotel/contracts": "workspace:*",
    "@catotel/i18n": "workspace:*",
    "@catotel/api-client": "^0.1.1",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.9",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "jose": "^6.1.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.553.0",
    "next": "^14.2.33",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.66.0",
    "zod": "^4.1.12",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "22.1.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "@eslint/js": "^9.18.0",
    "@next/eslint-plugin-next": "^14.2.5",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^16.0.0",
    "postcss": "8.4.39",
    "prettier": "^3.4.2",
    "start-server-and-test": "^2.1.2",
    "tailwindcss": "3.4.9",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
